 fill-rule=\"evenodd\" d=\"M14.85 3H1.15C.52 3 0 3.52 0 4.15v7.69C0 12.48.52 13 1.15 13h13.69c.64 0 1.15-.52 1.15-1.15v-7.7C16 3.52 15.48 3 14.85 3zM9 11H7V8L5.5 9.92 4 8v3H2V5h2l1.5 2L7 5h2v6zm2.99.5L9.5 8H11V5h2v3h1.5l-2.51 3.5z\"></path></svg>'         +\r\n                            '</a>'     +\r\n                        '</div>'     +\r\n                        '<div class=\"vcol vcol-70 text-right\">'         +\r\n                            `<button type=\"button\"  title=\"Cmd|Ctrl+Enter\" class=\"vsubmit vbtn\">${vm.i18n.t('submit')}</button>`     +\r\n                        '</div>' +\r\n                    '</div>' +\r\n                    '<div class=\"vemojis\" style=\"display:none;\"></div>' +\r\n                    '<div class=\"vinput vpreview\" style=\"display:none;\"></div>' +\r\n                '</div></div>'            + \r\n                `<div class=\"vcount\" style=\"display:none;\"><span class=\"vnum\">0</span> ${vm.i18n.t('comments')}</div>`            + \r\n                '<div class=\"vload-top text-center\" style=\"display:none;\"><i class=\"vspinner\" style=\"width:30px;height:30px;\"></i></div>' +\r\n                '<div class=\"vcards\"></div>'            + \r\n                '<div class=\"vload-bottom text-center\" style=\"display:none;\"><i class=\"vspinner\" style=\"width:30px;height:30px;\"></i></div>' +\r\n                '<div class=\"vempty\" style=\"display:none;\"></div>'            + \r\n                `<div class=\"vpage txt-center\" style=\"display:none\"><button type=\"button\" class=\"vmore vbtn\">${vm.i18n.t('more')}</button></div>`            + \r\n                `<div class=\"vpower txt-right\">Powered By <a href=\"https://valine.js.org\" target=\"_blank\">Valine</a><br>v${VERSION}</div>`;\r\n            vm.$el.html(vwrap);\r\n            vm.$el.find('.cancel-reply').on('click',e=>{\r\n                vm.reset()\r\n            })\r\n            // Empty Data\r\n            let vempty = vm.$el.find('.vempty');\r\n            vm.$nodata = {\r\n                show(txt) {\r\n                    vempty.html(txt || vm.i18n.t('sofa')).show()\r\n                    return vm;\r\n                },\r\n                hide() {\r\n                    vempty.hide()\r\n                    return vm;\r\n                }\r\n            }\r\n            // loading\r\n            let $LoadBarBottom = vm.$el.find('.vload-bottom'),\r\n                $loadBarTop = vm.$el.find('.vload-top');\r\n            vm.$loading = {\r\n                show(mt) {\r\n                    mt && $loadBarTop.show() || $LoadBarBottom.show()\r\n                    vm.$nodata.hide();\r\n                    return vm;\r\n                },\r\n                hide() {\r\n                    $loadBarTop.hide()\r\n                    $LoadBarBottom.hide()\r\n                    vm.$el.find('.vcard').length === 0 && vm.$nodata.show()\r\n                    return vm;\r\n                }\r\n            };\r\n        }\r\n        lcLoader(vm.cfg,e => {\r\n            // get comment count\r\n            let els = $('.valine-comment-count'),\r\n                currIdx = 0;\r\n            (function vccIterator(els){\r\n                let el = els[currIdx++];\r\n                if (el) {\r\n                    let k = $(el).attr('data-xid');\r\n                    !!k && vm.Q(k).count().then(n => {\r\n                        el.innerText = n\r\n                        vccIterator(els)\r\n                    }).catch(ex => {\r\n                        el.innerText = 0\r\n                    })\r\n                }\r\n            })(els)\r\n    \r\n            // Counter\r\n            visitor && CounterFactory.add(AV.Object.extend('Counter'),vm.cfg.path);\r\n            console.log(3)\r\n            // Bind Event\r\n            vm.$el && vm.bind();\r\n        })\r\n    } catch (ex) {\r\n        console.log(ex)\r\n        ErrHandler(vm,ex,'init')\r\n    }\r\n}\r\n\r\n// 新建Counter对象\r\nlet createCounter = function (Counter, o) {\r\n    let newCounter = new Counter();\r\n    let acl = new AV.ACL();\r\n    acl.setPublicReadAccess(true);\r\n    acl.setPublicWriteAccess(true);\r\n    newCounter.setACL(acl);\r\n    newCounter.set('url', o.url)\r\n    newCounter.set('xid', o.xid)\r\n    newCounter.set('title', o.title)\r\n    newCounter.set('time', 1)\r\n    newCounter.save().then(ret => {\r\n        $(o.el).find('.leancloud-visitors-count').text(1)\r\n    }).catch(ex => {\r\n        console.log(ex)\r\n    });\r\n}\r\nlet CounterFactory = {\r\n    add(Counter,currPath) {\r\n        let vm = this\r\n        let lvs = $('.leancloud_visitors,.leancloud-visitors');\r\n        if (lvs.length === 1) {\r\n            let lv = lvs[0];\r\n            let url = decodeURI($(lv).attr('id'));\r\n            let title = $(lv).attr('data-flag-title');\r\n            let xid = encodeURI(url);\r\n            let o = {\r\n                el: lv,\r\n                url: url,\r\n                xid: xid,\r\n                title: title\r\n            }\r\n            // 判断是否需要+1\r\n            if (decodeURI(url) === decodeURI(currPath)) {\r\n                let q = new AV.Query(Counter);\r\n                q.equalTo('url', url);\r\n                q.find().then(ret => {\r\n                    \r\n                    if (ret.length > 0) {\r\n                        let v = ret[0];\r\n                        v.increment(\"time\");\r\n                        v.save().then(rt => {\r\n                            $(lv).find( '.leancloud-visitors-count').text(rt.get('time'))\r\n                        }).catch(ex => {\r\n                            console.log(ex)\r\n                        });\r\n                    } else {\r\n                        createCounter(Counter, o)\r\n                    }\r\n                }).catch(ex => {\r\n                    console.log(ex)\r\n                    if(ex.code == 101) createCounter(Counter, o)\r\n                    else ErrHandler(vm,ex)\r\n                    \r\n                })\r\n            } else CounterFactory.show(Counter, lvs)\r\n        } else CounterFactory.show(Counter, lvs)\r\n    },\r\n    show(Counter, lvs) {\r\n        let COUNT_CONTAINER_REF = '.leancloud-visitors-count',urls = [];\r\n        // 重置所有计数\r\n        lvs.forEach(el=>{\r\n            let cel = $(el).find(COUNT_CONTAINER_REF);\r\n            if (cel) cel.text('0')\r\n            urls.push(/\\%/.test($(el).attr('id')) ? decodeURI($(el).attr('id')) : $(el).attr('id'))\r\n        })\r\n        if (urls.length) {\r\n            let query = new AV.Query(Counter);\r\n            query.containedIn('url', urls);\r\n            query.find().then(ret => {\r\n                if (ret.length > 0) {\r\n                // let els = $(`.leancloud_visitors,.leancloud-visitors`);\r\n                lvs.forEach(el=>{\r\n                    ret.forEach(item => {\r\n                        let url = item.get('xid') || encodeURI(item.get('url')),\r\n                            time = item.get('time'),\r\n                            $el = $(el),\r\n                            id = $el.attr('id')\r\n                            if((/\\%/.test(id) ? id : encodeURI(id)) == url){\r\n                                let cel = $el.find(COUNT_CONTAINER_REF);\r\n                                if (cel) cel.text(time)\r\n                            }\r\n                    })\r\n                })\r\n                }\r\n            }).catch(ex => {\r\n                console.log(ex)\r\n            })\r\n        }\r\n    }\r\n}\r\n\r\n/**\r\n * LeanCloud SDK Query Util\r\n * @param {String} url \r\n * @param {String} id\r\n */\r\nValineFactory.prototype.Q = function (k) {\r\n    let vm = this,\r\n        len = arguments.length,\r\n        className = vm['cfg']['clazzName'];\r\n    if (len == 1) {\r\n        let notExist = new AV.Query(className);\r\n        notExist.doesNotExist('rid');\r\n        let isEmpty = new AV.Query(className);\r\n        isEmpty.equalTo('rid', '');\r\n        let q = AV.Query.or(notExist, isEmpty);\r\n        if (k === '*') q.exists('url');\r\n        else q.equalTo('url', decodeURI(k));\r\n        q.addDescending('createdAt');\r\n        q.addDescending('insertedAt');\r\n        return q;\r\n    } else {\r\n        let ids = JSON.stringify(arguments[1]).replace(/(\\[|\\])/g, '');\r\n        let cql = `select * from ${className} where rid in (${ids}) order by -createdAt,-createdAt`;\r\n        return AV.Query.doCloudQuery(cql)\r\n    }\r\n}\r\n\r\n/**\r\n * install Multi language support\r\n * @param {String} key langName\r\n * @param {Object} mode langSource\r\n */\r\nValineFactory.prototype.installLocale = function (key, mode) {\r\n    let vm = this;\r\n    vm.i18n(key, mode)\r\n    return vm;\r\n}\r\n\r\n/**\r\n * \r\n * @param {String} path \r\n */\r\nValineFactory.prototype.setPath = function(v) {\r\n    this.config.path = v\r\n    return this\r\n}\r\n\r\n/**\r\n * Bind Event\r\n */\r\nValineFactory.prototype.bind = function () {\r\n    let vm = this;\r\n    // load emojis\r\n    let _vemojis = vm.$el.find('.vemojis'),\r\n        _vpreview = vm.$el.find('.vpreview'),\r\n        _emojiCtrl = vm.$el.find('.vemoji-btn'),\r\n        _vpreviewCtrl = vm.$el.find(`.vpreview-btn`),\r\n        _veditor = vm.$el.find('.veditor'),\r\n        emojiData = Emoji.maps,\r\n        emojiLoaded = false,\r\n        loadEmojiEvt = e => {\r\n            let _is = []\r\n            for (let key in emojiData) {\r\n                if (emojiData.hasOwnProperty(key)) {\r\n                    !!Emoji.build(key) && _is.push(`<i title=\"${key}\" >${Emoji.build(key)}</i>`)\r\n                }\r\n            }\r\n            _vemojis.html(_is.join(''));\r\n            emojiLoaded = true;\r\n            _vemojis.find('i').on('click',function(e){\r\n                e.preventDefault()\r\n                _insertAtCaret(_veditor[0], ` :${$(this).attr('title')}:`)\r\n            })\r\n        };\r\n    vm.$emoji = {\r\n        show() {\r\n            !emojiLoaded && loadEmojiEvt()\r\n            vm.$preview.hide();\r\n            _vemojis.show();\r\n            _emojiCtrl.addClass('actived');\r\n            return vm.$emoji\r\n        },\r\n        hide() {\r\n            _emojiCtrl.removeClass('actived');\r\n            _vemojis.hide();\r\n            return vm.$emoji\r\n        }\r\n    }\r\n    vm.$preview = {\r\n        show() {\r\n            if (TEMP_COMMENT_CTX) {\r\n                vm.$emoji.hide();\r\n                _vpreviewCtrl.addClass('actived');\r\n                _vpreview.html(TEMP_COMMENT_CTX).show()\r\n                _activeOtherFn()\r\n            }else vm.$preview.hide()\r\n            return vm.$preview\r\n        },\r\n        hide() {\r\n            _vpreviewCtrl.removeClass('actived');\r\n            _vpreview.hide().html('')\r\n            return vm.$preview\r\n        }\r\n    }\r\n\r\n    /**\r\n     * 评论框内容变化事件\r\n     * @param {HTMLElement} el \r\n     */\r\n    let syncContentEvt = $el => {\r\n        let TEMP_CTX = md($el.val() || '')\r\n        if(!TEMP_CTX) vm.$preview.hide()\r\n        if(TEMP_COMMENT_CTX == TEMP_CTX) return\r\n        TEMP_COMMENT_CTX = TEMP_CTX\r\n        _vpreviewCtrl.hasClass('actived') > -1 && TEMP_COMMENT_CTX !=_vpreview.html() && (_vpreview.html(TEMP_COMMENT_CTX)) ;\r\n        autosize($el[0])\r\n        _activeOtherFn()\r\n    }\r\n    _emojiCtrl.on('click',e => {\r\n        _emojiCtrl.hasClass('actived') ? vm.$emoji.hide() : vm.$emoji.show()\r\n    })\r\n    _vpreviewCtrl.on('click',e => {\r\n        _vpreviewCtrl.hasClass('actived') ? vm.$preview.hide() : vm.$preview.show()\r\n    })\r\n    let meta = vm.cfg.meta;\r\n    let inputs = {};\r\n\r\n    // 同步操作\r\n    let inputMapping = {\r\n        veditor: \"comment\"\r\n    }\r\n    meta.forEach(item=>{\r\n        inputMapping[`v${item}`] = item;\r\n    })\r\n    for (let i in inputMapping) {\r\n        if (inputMapping.hasOwnProperty(i)) {\r\n            let _v = inputMapping[i];\r\n            let $el = vm.$el.find(`.${i}`);\r\n            inputs[_v] = $el;\r\n            $el.on('input change blur propertychange',e => {\r\n                if(vm.cfg.enableQQ && e.type === 'blur' && _v === 'nick'){\r\n                    if(!isNaN($el.val())) {\r\n                        fetchQQFn($el.val(),ret=>{\r\n                            let name = ret['nick'] || $el.val(),\r\n                                qqMail = ret['qq']+'@qq.com'\r\n                            $('.vnick').val(name)\r\n                            $('.vmail').val(qqMail)\r\n                            defaultComment['nick']= name\r\n                            defaultComment['mail']= qqMail\r\n                            defaultComment['QQAvatar'] = ret['pic']\r\n                        })\r\n                    } else {\r\n                        if($.store.get(QQCacheKey) && $.store.get(QQCacheKey)['nick'] != $el.val()){\r\n                            $.store.remove(QQCacheKey)\r\n                            defaultComment['nick']= $el.val()\r\n                            defaultComment['mail']= ''\r\n                            defaultComment['QQAvatar'] = ''\r\n                        }\r\n                    }\r\n                }\r\n                if (_v === 'comment') syncContentEvt($el)\r\n                else defaultComment[_v] = $.escape($el.val().replace(/(^\\s*)|(\\s*$)/g, \"\")).substring(0,40);\r\n            });\r\n        }\r\n    }\r\n    let _insertAtCaret = (field, val) => {\r\n        if (document.selection) {\r\n            //For browsers like Internet Explorer\r\n            field.focus();\r\n            let sel = document.selection.createRange();\r\n            sel.text = val;\r\n            field.focus();\r\n        } else if (field.selectionStart || field.selectionStart == '0') {\r\n            //For browsers like Firefox and Webkit based\r\n            let startPos = field.selectionStart;\r\n            let endPos = field.selectionEnd;\r\n            let scrollTop = 