st = new AV.Query(className);\r\n        notExist.doesNotExist('rid');\r\n        let isEmpty = new AV.Query(className);\r\n        isEmpty.equalTo('rid', '');\r\n        let q = AV.Query.or(notExist, isEmpty);\r\n        if (k === '*') q.exists('url');\r\n        else q.equalTo('url', decodeURI(k));\r\n        q.addDescending('createdAt');\r\n        q.addDescending('insertedAt');\r\n        return q;\r\n    } else {\r\n        let ids = JSON.stringify(arguments[1]).replace(/(\\[|\\])/g, '');\r\n        let cql = `select * from ${className} where rid in (${ids}) order by -createdAt,-createdAt`;\r\n        return AV.Query.doCloudQuery(cql)\r\n    }\r\n}\r\n\r\n/**\r\n * install Multi language support\r\n * @param {String} key langName\r\n * @param {Object} mode langSource\r\n */\r\nValineFactory.prototype.installLocale = function (key, mode) {\r\n    let vm = this;\r\n    vm.i18n(key, mode)\r\n    return vm;\r\n}\r\n\r\n/**\r\n * \r\n * @param {String} path \r\n */\r\nValineFactory.prototype.setPath = function(v) {\r\n    this.config.path = v\r\n    return this\r\n}\r\n\r\n/**\r\n * Bind Event\r\n */\r\nValineFactory.prototype.bind = function () {\r\n    let vm = this;\r\n    // load emojis\r\n    let _vemojis = vm.$el.find('.vemojis'),\r\n        _vpreview = vm.$el.find('.vpreview'),\r\n        _emojiCtrl = vm.$el.find('.vemoji-btn'),\r\n        _vpreviewCtrl = vm.$el.find(`.vpreview-btn`),\r\n        _veditor = vm.$el.find('.veditor'),\r\n        emojiData = Emoji.maps,\r\n        emojiLoaded = false,\r\n        loadEmojiEvt = e => {\r\n            let _is = []\r\n            for (let key in emojiData) {\r\n                if (emojiData.hasOwnProperty(key)) {\r\n                    !!Emoji.build(key) && _is.push(`<i title=\"${key}\" >${Emoji.build(key)}</i>`)\r\n                }\r\n            }\r\n            _vemojis.html(_is.join(''));\r\n            emojiLoaded = true;\r\n            _vemojis.find('i').on('click',function(e){\r\n                e.preventDefault()\r\n                _insertAtCaret(_veditor[0], ` :${$(this).attr('title')}:`)\r\n            })\r\n        };\r\n    vm.$emoji = {\r\n        show() {\r\n            !emojiLoaded && loadEmojiEvt()\r\n            vm.$preview.hide();\r\n            _vemojis.show();\r\n            _emojiCtrl.addClass('actived');\r\n            return vm.$emoji\r\n        },\r\n        hide() {\r\n            _emojiCtrl.removeClass('actived');\r\n            _vemojis.hide();\r\n            return vm.$emoji\r\n        }\r\n    }\r\n    vm.$preview = {\r\n        show() {\r\n            if (TEMP_COMMENT_CTX) {\r\n                vm.$emoji.hide();\r\n                _vpreviewCtrl.addClass('actived');\r\n                _vpreview.html(TEMP_COMMENT_CTX).show()\r\n                _activeOtherFn()\r\n            }else vm.$preview.hide()\r\n            return vm.$preview\r\n        },\r\n        hide() {\r\n            _vpreviewCtrl.removeClass('actived');\r\n            _vpreview.hide().html('')\r\n            return vm.$preview\r\n        }\r\n    }\r\n\r\n    /**\r\n     * 评论框内容变化事件\r\n     * @param {HTMLElement} el \r\n     */\r\n    let syncContentEvt = $el => {\r\n        let TEMP_CTX = md($el.val() || '')\r\n        if(!TEMP_CTX) vm.$preview.hide()\r\n        if(TEMP_COMMENT_CTX == TEMP_CTX) return\r\n        TEMP_COMMENT_CTX = TEMP_CTX\r\n        _vpreviewCtrl.hasClass('actived') > -1 && TEMP_COMMENT_CTX !=_vpreview.html() && (_vpreview.html(TEMP_COMMENT_CTX)) ;\r\n        autosize($el[0])\r\n        _activeOtherFn()\r\n    }\r\n    _emojiCtrl.on('click',e => {\r\n        _emojiCtrl.hasClass('actived') ? vm.$emoji.hide() : vm.$emoji.show()\r\n    })\r\n    _vpreviewCtrl.on('click',e => {\r\n        _vpreviewCtrl.hasClass('actived') ? vm.$preview.hide() : vm.$preview.show()\r\n    })\r\n    let meta = vm.cfg.meta;\r\n    let inputs = {};\r\n\r\n    // 同步操作\r\n    let inputMapping = {\r\n        veditor: \"comment\"\r\n    }\r\n    meta.forEach(item=>{\r\n        inputMapping[`v${item}`] = item;\r\n    })\r\n    for (let i in inputMapping) {\r\n        if (inputMapping.hasOwnProperty(i)) {\r\n            let _v = inputMapping[i];\r\n            let $el = vm.$el.find(`.${i}`);\r\n            inputs[_v] = $el;\r\n            $el.on('input change blur propertychange',e => {\r\n                if(vm.cfg.enableQQ && e.type === 'blur' && _v === 'nick'){\r\n                    if(!isNaN($el.val())) {\r\n                        fetchQQFn($el.val(),ret=>{\r\n                            let name = ret['nick'] || $el.val(),\r\n                                qqMail = ret['qq']+'@qq.com'\r\n                            $('.vnick').val(name)\r\n                            $('.vmail').val(qqMail)\r\n                            defaultComment['nick']= name\r\n                            defaultComment['mail']= qqMail\r\n                            defaultComment['QQAvatar'] = ret['pic']\r\n                        })\r\n                    } else {\r\n                        if($.store.get(QQCacheKey) && $.store.get(QQCacheKey)['nick'] != $el.val()){\r\n                            $.store.remove(QQCacheKey)\r\n                            defaultComment['nick']= $el.val()\r\n                            defaultComment['mail']= ''\r\n                            defaultComment['QQAvatar'] = ''\r\n                        }\r\n                    }\r\n                }\r\n                if (_v === 'comment') syncContentEvt($el)\r\n                else defaultComment[_v] = $.escape($el.val().replace(/(^\\s*)|(\\s*$)/g, \"\")).substring(0,40);\r\n            });\r\n        }\r\n    }\r\n    let _insertAtCaret = (field, val) => {\r\n        if (document.selection) {\r\n            //For browsers like Internet Explorer\r\n            field.focus();\r\n            let sel = document.selection.createRange();\r\n            sel.text = val;\r\n            field.focus();\r\n        } else if (field.selectionStart || field.selectionStart == '0') {\r\n            //For browsers like Firefox and Webkit based\r\n            let startPos = field.selectionStart;\r\n            let endPos = field.selectionEnd;\r\n            let scrollTop = field.scrollTop;\r\n            field.value = field.value.substring(0, startPos) + val + field.value.substring(endPos, field.value.length);\r\n            field.focus();\r\n            field.selectionStart = startPos + val.length;\r\n            field.selectionEnd = startPos + val.length;\r\n            field.scrollTop = scrollTop;\r\n        } else {\r\n            field.focus();\r\n            field.value += val;\r\n        }\r\n        setTimeout(x=>{\r\n            syncContentEvt($(field))\r\n        },100)\r\n        // autosize(field)\r\n    }\r\n    let PAGE = {\r\n            no:1,\r\n            size: vm.cfg.pageSize,\r\n            skip: vm.cfg.pageSize,\r\n        },\r\n        $moreBtn = vm.$el.find('.vpage');\r\n    \r\n    $moreBtn.on('click', e => {\r\n        $moreBtn.hide();\r\n        PAGE['no']++\r\n        query();\r\n    })\r\n    let query = () => {\r\n        let size = PAGE['size'],\r\n            no = PAGE['no'],\r\n            count = Number(vm.$el.find('.vnum').text());\r\n        vm.$loading.show();\r\n        let cq = vm.Q(vm.cfg.path);\r\n        cq.limit(size);\r\n        cq.skip((no - 1) * size);\r\n        cq.find().then(rets => {\r\n            PAGE['skip'] = PAGE['size'];\r\n            if(rets && rets.length){\r\n                let rids = []\r\n                rets.forEach(ret=>{\r\n                    rids.push(ret.id)\r\n                    insertDom(ret, vm.$el.find('.vcards'), !0)\r\n                })\r\n                // load children comment\r\n                vm.Q(vm.cfg.path, rids).then(ret => {\r\n                    let kids = ret && ret.results || []\r\n                    kids.forEach(child=>{\r\n                        insertDom(child, $(`.vquote[data-self-id=\"${child.get('rid')}\"]`))\r\n                        // console.log(vm.$el.find(`.vcard[id=\"${child.id}\"]>.vquote:first`))\r\n                    }).catch(ex=>{\r\n                        console.log(ex)\r\n                    })\r\n                })\r\n                size * no < count ? $moreBtn.show() : $moreBtn.hide() ;\r\n                _activeOtherFn()\r\n            }else vm.$nodata.show()\r\n            vm.$loading.hide();\r\n        }).catch(ex => {\r\n            console.log(ex)\r\n            vm.$loading.hide()\r\n            ErrHandler(vm,ex,'query')\r\n        })\r\n    }\r\n    vm.Q(vm.cfg.path).count().then(num => {\r\n        if (num > 0) {\r\n            vm.$el.find('.vcount').show().find('.vnum').text(num);\r\n            query();\r\n        } else {\r\n            vm.$loading.hide();\r\n        }\r\n    }).catch(ex => {\r\n        console.log(4)\r\n        ErrHandler(vm,ex,'count')\r\n        return\r\n    });\r\n\r\n    let insertDom = (rt, node, mt) => {\r\n        let $vcard = $(`<div class=\"vcard\" id=\"${rt.id}\"></div>`),\r\n            ua = rt.get('ua'), uaMeta='';\r\n        if (!!ua && !/ja/.test(vm.cfg.lang)) {\r\n            ua = $.detect(ua);\r\n            uaMeta = `<span class=\"vsys\">${ua.browser} ${ua.version}</span> <span class=\"vsys\">${ua.os} ${ua.osVersion}</span>`;\r\n        }\r\n        if(vm.cfg.path === '*') uaMeta = `<a href=\"${rt.get('url')}\" class=\"vsys\">${rt.get('url')}</a>`\r\n        let _t = rt.get('link')?(/^https?\\:\\/\\//.test(rt.get('link')) ? rt.get('link') : 'http://'+rt.get('link')) : '',\r\n            _nick = _t ? `<a class=\"vnick\" rel=\"nofollow\" href=\"${_t}\" target=\"_blank\" >${rt.get(\"nick\")}</a>` : `<span class=\"vnick\">${rt.get('nick')}</span>`,\r\n            _img = _avatarSetting['hide'] \r\n                ? '' \r\n                : ((vm.cfg.enableQQ && rt.get('QQAvatar')) \r\n                ? `<img class=\"vimg\" src=\"${rt.get('QQAvatar')}\" referrerPolicy=\"no-referrer\"/>` \r\n                : `<img class=\"vimg\" src=\"${_avatarSetting['cdn']+md5(rt.get('mail'))+_avatarSetting['params']}\">`);\r\n            \r\n        let html = \r\n            `${_img}`+\r\n            `<div class=\"vh\">`+\r\n                `<div class=\"vhead\">${_nick} ${uaMeta}</div>`+\r\n                '<div class=\"vmeta\">'+\r\n                    `<span class=\"vtime\" >${timeAgo(rt.get('insertedAt'),vm.i18n)}</span>`+\r\n                    `<span class=\"vat\" data-vm-id=\"${rt.get('rid')||rt.id}\" data-self-id=\"${rt.id}\">${vm.i18n.t('reply')}</span>`+\r\n                '</div>'+\r\n                `<div class=\"vcontent\" data-expand=\"${vm.i18n.t('expand')}\">${xss(rt.get(\"comment\"))}</div>`+\r\n                `<div class=\"vreply-wrapper\" data-self-id=\"${rt.id}\"></div>`+\r\n                `<div class=\"vquote\" data-self-id=\"${rt.id}\"></div>`+\r\n            `</div>`;\r\n        $vcard.html(html)\r\n        let _vat = $vcard.find('.vat');\r\n        $vcard.find('a').forEach(a=>{\r\n            if(a && !$(a).hasClass('at')){\r\n                $(a).attr({\r\n                    'target': '_blank',\r\n                    'rel': 'noopener'\r\n                })\r\n            }\r\n        })\r\n        if (mt) node.append($vcard);\r\n        else node.prepend($vcard) //node[0].insertBefore($vcard[0], _vlis[0]);\r\n        let _vcontent = $vcard.find('.vcontent');\r\n        if (_vcontent) expandEvt(_vcontent);\r\n        if (_vat) bindAtEvt(_vat, rt);\r\n    }\r\n\r\n    let atData = {}\r\n    // at event\r\n    let bindAtEvt = ($el, rt) => {\r\n        $el.on('click',e=>{\r\n            let vmId =$el.attr('data-vm-id'),\r\n                selfId =$el.attr('data-self-id'),\r\n                editor = vm.$el.find('.vwrap'),\r\n                at = `@${$.escape(rt.get('nick'))}`;\r\n            $(`.vreply-wrapper[data-self-id=\"${selfId}\"]`).append(editor).find('.cancel-reply').show();\r\n            atData = {\r\n                'at': $.escape(at) + ' ',\r\n                'rid': vmId,\r\n                'pid': selfId,\r\n                'rmail': rt.get('mail'),\r\n            }\r\n            inputs['comment'].attr({'placeholder':at})[0].focus();\r\n        })\r\n    }\r\n\r\n\r\n    let _activeOtherFn = () => {\r\n        setTimeout(function () {\r\n            try {\r\n                if (vm.cfg.mathjax && ('MathJax' in window) && ('version' in window['MathJax'])) /^3.*/.test(window['MathJax']['version']) &&  MathJax.typeset() || MathJax.Hub.Queue([\"Typeset\", MathJax.Hub, document.querySelector(\".v\")])\r\n                if('renderMathInElement' in window) renderMathInElement($('.v')[0],{\r\n                    delimiters:[\r\n                        {left: '$$', right: '$$', display: true},\r\n                        {left: '$', right: '$', display: false}\r\n                    ]\r\n                })\r\n            } catch (ex) {}\r\n        }, 100)\r\n    }\r\n\r\n    // expand event\r\n    let expandEvt = $el => {\r\n        setTimeout(function () {\r\n            if ($el[0].offsetHeight > 200) {\r\n                $el.addClass('expand');\r\n                $el.on('click',e => {\r\n                    $el.removeClass('expand');\r\n                })\r\n            }\r\n        })\r\n    }\r\n\r\n    // cache\r\n    let getCache = s => {\r\n        s = $.store.get(MetaCacheKey) || s;\r\n        if (s) {\r\n            for (let i in meta) {\r\n                let k = meta[i];\r\n                vm.$el.find(`.v${k}`).val($.unescape(s[k]));\r\n                defaultComment[k] = s[k];\r\n            }\r\n        }\r\n        let x = $.store.get(QQCacheKey)\r\n        defaultComment['QQAvatar'] = vm.cfg.enableQQ && !!x && x['pic'] || ''\r\n    }\r\n    getCache();\r\n    // reset form\r\n    vm.reset = () => {\r\n        defaultComment['comment'] = \"\";\r\n        inputs['comment'].val(\"\");\r\n        syncContentEvt(inputs['comment'])\r\n        inputs['comment'].attr('placeholder', vm.cfg.placeholder);\r\n        atData = {};\r\n        vm.$preview.hide();\r\n        vm.$el.find('.v